{
  "name": "sss-ui-plus",
  "main": "lib/index.js",
  "module": "es/index.mjs",
  "types": "es/index.d.ts",
  "style": "dist/index.css",
  "private": false,
  "sideEffects": false,
  "version": "0.2.0-beta.17",
  "type": "module",
  "author": {
    "name": "laster",
    "email": "lasterxin@163.com"
  },
  "description": "适用于vue3的组件库",
  "keywords": [
    "UI",
    "Vue3",
    "typescript"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "docs:dev": "vitepress dev ./docs/ --host 0.0.0.0",
    "docs:build": "vitepress build ./docs/",
    "type:dev": "node buildWebTypes.js",
    "type build": "vue-docgen-web-types"
  },
  "web-types": " ./web-types.json",
  "vetur": {
    "tags": "./tags.json",
    "attributes": " ./attributes.json"
  },
  "workspaces": [
    "packages/*",
    "docs"
  ],
  "dependencies": {
    "@floating-ui/vue": "^1.0.2",
    "@vue/runtime-core": "^3.3.4",
    "@vueuse/core": "^10.2.1",
    "vue": "^3.3.4",
    "vue-docgen-web-types": "^0.1.8"
  },
  "devDependencies": {
    "@sss-ui-plus/components": "workspace:*",
    "@sss-ui-plus/hooks": "workspace:*",
    "@sss-ui-plus/styles": "workspace:*",
    "@sss-ui-plus/typings": "workspace:*",
    "@sss-ui-plus/utils": "workspace:*",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue-macros/volar": "^0.12.2",
    "autoprefixer": "^10.4.14",
    "components-helper": "^2.1.4",
    "less-loader": "^11.1.3",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.35.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "unplugin-vue-define-options": "^1.3.11",
    "vite": "^4.4.0",
    "vite-plugin-dts": "^3.3.1",
    "vue-tsc": "^1.8.3"
  }
}
